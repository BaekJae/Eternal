#include <stdio.h>
#pragma warning(disable:4996)

int main() {
	int arr[10] = { 45,89,13,52,6,71,20,74,56,49 };
	int j, k, temp, count=0;
	printf("교환 전\n->");
	for (j = 0; j < 10; j++)
	{
		printf("%d ", arr[j]);
	}
	for (j = 0; j < 10; j++)
	{
		for (k = j; k <10; k++) {
			if (arr[j] > arr[k]) {
				temp = arr[j];
				arr[j] = arr[k];
				arr[k] = temp;
			}
			count++;
		}
	}
	printf("\n교환 후\n->");
	for (j = 0; j < 10; j++) 
	{
		printf("%d ", arr[j]);
	}
	printf("\n비교 횟수는 %d 입니다.\n", count);
}
