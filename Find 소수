#include <stdio.h>
#pragma warning(disable:4996)

int main()
{

  int N, num, I, j, count=0;
  printf(“몇까지의 소수를 구하시겠습니까?\n”);
  while(1){
      
    scanf(“%d”, &N);
	if(N==0)	
	  break;

       for(I=2; I<=N; I++)
       {
          count=0;
          for(j=1; j<=I; j++)
          {
             if (i%j==0)
             {
                count++;
             }
          }
          if(count==2)
          {
             printf(“%d”, I);
          }
       }
       printf(“\n 몇까지의 소수를 구하시겠습니까?\n”);
     }
}
